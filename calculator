import java.lang.*;
import java.util.Scanner;
class Main
{
    public static void main (String[] args) throws java.lang.Exception {
        Scanner in = new Scanner(System.in);
        while (in.hasNext()){
        String s1 = in.next();
        int a = in.nextInt();
        int b = in.nextInt();
        switch (s1) {
            case "+":
                System.out.println(a + b);
                break;
            case "-":
                System.out.println(a - b);
                break;
            case "*":
                System.out.println(a * b);
                break;
            case "/":
                System.out.println(a / b);
                break;
            case "%":
                System.out.println(a % b);
                break;

            }
        }
    }
}
